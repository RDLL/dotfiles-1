! Black color that will not be affected by bold highlighting.
*.color66: #141012
*color66:  #141012

! Xclock colors.
XClock*foreground: #edeae5
XClock*background: #141012
XClock*majorColor:  rgba:ed/ea/e5/ff
XClock*minorColor:  rgba:ed/ea/e5/ff
XClock*hourColor:   rgba:ed/ea/e5/ff
XClock*minuteColor: rgba:ed/ea/e5/ff
XClock*secondColor: rgba:ed/ea/e5/ff

! Set depth to make transparency work.
URxvt*depth: 32
